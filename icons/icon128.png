<canvas id="icon128" width="128" height="128"></canvas>
<script>
  const canvas = document.getElementById('icon128');
  const ctx = canvas.getContext('2d');
  
  // Background with subtle pattern
  const gradient = ctx.createLinearGradient(0, 0, 128, 128);
  gradient.addColorStop(0, '#10a37f');
  gradient.addColorStop(0.5, '#0c8b6f');
  gradient.addColorStop(1, '#0a4d3c');
  
  ctx.fillStyle = gradient;
  ctx.fillRect(0, 0, 128, 128);
  
  // Main icon: Navigation path
  ctx.strokeStyle = '#ffffff';
  ctx.lineWidth = 6;
  ctx.lineCap = 'round';
  
  ctx.beginPath();
  ctx.moveTo(30, 30);
  ctx.lineTo(60, 30);
  ctx.lineTo(60, 60);
  ctx.lineTo(90, 60);
  ctx.lineTo(90, 90);
  ctx.lineTo(98, 98);
  ctx.stroke();
  
  // Conversation nodes
  ctx.fillStyle = '#ffffff';
  ctx.beginPath();
  ctx.arc(30, 30, 10, 0, Math.PI * 2); // Start
  ctx.fill();
  
  ctx.beginPath();
  ctx.arc(90, 90, 8, 0, Math.PI * 2); // Middle
  ctx.fill();
  
  ctx.beginPath();
  ctx.arc(98, 98, 6, 0, Math.PI * 2); // End
  ctx.fill();
  
  // Text
  ctx.fillStyle = '#ffffff';
  ctx.font = 'bold 20px Arial';
  ctx.textAlign = 'center';
  ctx.fillText('PromptPath', 64, 115);
</script>